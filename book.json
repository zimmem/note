{
     
    "generator": "site", 
    "title": "note by zimmem",
    "description": "notebook ",   
    "extension": null,

   
    "plugins": [
      "alerts",
      "expandable-chapters-interactive",
       "toc",
       "-lunr", "-search", "search-plus-mod",
       "page-numbering"
      ],

   
    "pluginsConfig": {
        "toc": {
            "addClass": true,
            "className": "toc"
        },
        "fontSettings": {
            "theme": "sepia",
            "family": "serif" ,
            "size": 1
        },
        "page-numbering": {
            "skipReadme": false,
            "chapterFormat": "Chapter #chapno#&nbsp;&nbsp;&nbsp;#title# ||| #chapno#&nbsp;&nbsp;&nbsp;#title#",
            "forceMultipleParts": false
        }
    },

   
    "variables": {},

   
    "links": {
        
        "sidebar": {
            "Custom link name": "https://zimmem.com"
        },

       
        "sharing": {
            "google": null,
            "facebook": null,
            "twitter": null,
            "weibo": null,
            "all": null
        }
    },


   
    "pdf": {
       
        "pageNumbers": false,

       
        "fontSize": 12,

       
        "paperSize": "a4",

       
        "margin": {
            "right": 62,
            "left": 62,
            "top": 36,
            "bottom": 36
        },

       
        "headerTemplate": null,

       
        "footerTemplate": null
    }
}